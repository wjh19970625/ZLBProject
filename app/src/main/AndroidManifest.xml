<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wjh.zhilibaoproject">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    //读写SD卡权限
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEM"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.example.wjh.zhilibaoproject.ui.MyApplication">
        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.SplashActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.MainActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.LoginActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.RegisterActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.AboutUsActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.PersonalDataActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.UpdatePersonalDataActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.ArticleDetailModeActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.FeedBackActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.MoreActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.ChangePasswordActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.ProductDetailActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.ForgetPasswordActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.AuthenticationActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.MarketDetailModeActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.OrderModeActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.SubmitOrderActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity android:name="com.example.wjh.zhilibaoproject.ui.activity.PayActivity" android:screenOrientation="userPortrait" android:theme="@style/NoTitleTheme"></activity>

        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind">
        </activity>
    </application>

</manifest>
